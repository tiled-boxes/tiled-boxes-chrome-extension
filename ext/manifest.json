{
  "name": "Tiled Boxes",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Tiled Boxes WebSocket Connector",
  "homepage_url": "https://github.com/tiled-boxes",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/socket.io.js",
      "src/options_custom/lib/store.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "permissions": [
    "tabs",
    "identity",
    "identity.email"
  ]
}
